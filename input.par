/*
 * Filenames
 */
runname		= test_run;					// (str) What you want your output files to be labeled as
moldatfile	= data/moldat/hcn.dat;		// (str) File must be located in "data/moldat/"
girdatfile	= data/girdat/g_hcn_1au.dat;	// (str) File must be located in "data/girdat/"

/*
 * Basic parameters. See cheat sheet for details.
 */
vexp		= 700.;		// (double)
Qwater		= 1e27;		// (double)
tkin		= 50.;		// (double)
abund		= 0.001;	// (double)
betamol		= 1.5e-5;	// (double)

rhelio		= 1.0;		// (double)
delta		= 1.0;		// (double) is related to img[0].distance
collPartIds	= 1;		// (int)


/*
 * Definitions for image #0. Add blocks for additional images.
 */
unit		= 0;		// (int) 0:Kelvin 1:Jansky/pixel 2:SI 3:Lsun/pixel 4:tau
trans		= 3;    	// (int) zero-indexed J quantum number
velres		= 100.;		// (double) Channel resolution in m/s
nchan		= 50;		// (int) Number of channels
pxls		= 256;		// (int) Pixels per dimension
imgres		= 0.5;		// (double) Resolution in arc seconds

/*
 * Optional parameters.
 */
// dopplerb	= 100;		// (double): default is 0
// useEP 		= 0;		// (int) – optional: default value is 0
// xne 		= 0.2;		// (double) – optional: default value is 0.2
